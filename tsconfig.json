{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true,
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "rootDir": "src",
    "paths": {
      "#prisma/*": ["./prisma/*", "./dist/prisma/*"]
    },
    "resolveJsonModule": true
  }
}
